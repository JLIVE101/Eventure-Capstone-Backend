//get express app object


var app = require('../app');
var http = require('http');
//set the port number


//start the server
var server = http.createServer(app);

/**
 * Listen on provided port, on all network interfaces.
 */

server.listen(app.get('port'), '127.0.0.1');
console.log('API Server listening on port ' + '127.0.0.1:' + app.get('port'));

